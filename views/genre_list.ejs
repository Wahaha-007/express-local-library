<%- include('layout-top'); -%>
<h1><%=title%></h1>
<ul>

    <% if(genre_list.length> 0) { %>
    <%    genre_list.forEach(genre=>{ %>

    <li><a href="<%=genre.url%>">
            <%=genre.name%>
        </a>
    </li>
    <%} )%>

    <% } else {%>
    <p>-- No genre to display --</p>
    <% }%>

</ul>
<%- include('layout-end'); -%>